{
  "name": "@ltodorov/primes",
  "version": "2.0.1",
  "description": "Utility functions for working with prime numbers",
  "type": "module",
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.30.0+sha512.2b5753de015d480eeb88f5b5b61e0051f05b4301808a82ec8b840c9d2adf7748eb352c83f5c1593ca703ff1017295bc3fdd3119abb9686efc96b9fcb18200937",
  "main": "./dist/index.js",
  "bin": "./index.js",
  "scripts": {
    "clean": "pnpm run /^clean:.+/",
    "clean:build": "rimraf ./dist",
    "clean:test": "rimraf ./coverage",
    "build": "pnpm clean:build && tsc",
    "lint": "pnpm biome lint --write .",
    "format": "pnpm biome format --write .",
    "check": "pnpm biome check --write .",
    "check:ci": "pnpm biome ci .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ltodorov/primes.git"
  },
  "bugs": {
    "url": "https://github.com/ltodorov/primes/issues"
  },
  "homepage": "https://github.com/ltodorov/primes",
  "keywords": [
    "typescript",
    "javascript",
    "nodejs",
    "prime numbers"
  ],
  "author": "Lyubomir Todorov",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@inquirer/testing": "^3.2.0",
    "@types/node": "^24.10.13",
    "@vitest/coverage-v8": "^4.0.18",
    "lefthook": "^2.1.1",
    "rimraf": "^6.1.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@inquirer/input": "^5.0.7",
    "@inquirer/select": "^5.0.7",
    "chalk": "^5.6.2"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "lefthook"
    ]
  }
}
