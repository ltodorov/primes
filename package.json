{
  "name": "@ltodorov/primes",
  "version": "2.0.1",
  "description": "Utility functions for working with prime numbers",
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6",
  "main": "./dist/index.js",
  "bin": "./index.js",
  "scripts": {
    "clean": "pnpm run /^clean:.+/",
    "clean:build": "rimraf ./dist",
    "clean:test": "rimraf ./coverage",
    "build": "pnpm clean:build && tsc",
    "lint": "pnpm biome lint --write .",
    "format": "pnpm biome format --write .",
    "check": "pnpm biome check --write .",
    "check:ci": "pnpm biome ci .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ltodorov/primes.git"
  },
  "bugs": {
    "url": "https://github.com/ltodorov/primes/issues"
  },
  "homepage": "https://github.com/ltodorov/primes",
  "keywords": [
    "typescript",
    "javascript",
    "nodejs",
    "prime numbers"
  ],
  "author": "Lyubomir Todorov",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@inquirer/testing": "^3.0.4",
    "@types/node": "^24.10.9",
    "@vitest/coverage-v8": "^4.0.17",
    "lefthook": "^2.0.15",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@inquirer/input": "^5.0.4",
    "@inquirer/select": "^5.0.4",
    "chalk": "^5.6.2"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "lefthook"
    ]
  }
}
