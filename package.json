{
  "name": "@ltodorov/primes",
  "version": "2.0.1",
  "description": "Utility functions for working with prime numbers",
  "type": "module",
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc",
  "main": "./dist/index.js",
  "bin": "./index.js",
  "scripts": {
    "clean": "pnpm run /^clean:.+/",
    "clean:build": "rimraf ./dist",
    "clean:test": "rimraf ./coverage",
    "build": "pnpm clean:build && tsc",
    "lint": "pnpm biome lint --write .",
    "format": "pnpm biome format --write .",
    "check": "pnpm biome check --write .",
    "check:ci": "pnpm biome ci .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ltodorov/primes.git"
  },
  "bugs": {
    "url": "https://github.com/ltodorov/primes/issues"
  },
  "homepage": "https://github.com/ltodorov/primes",
  "keywords": [
    "typescript",
    "javascript",
    "nodejs",
    "prime numbers"
  ],
  "author": "Lyubomir Todorov",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@inquirer/testing": "^3.0.4",
    "@types/node": "^24.10.13",
    "@vitest/coverage-v8": "^4.0.18",
    "lefthook": "^2.1.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@inquirer/input": "^5.0.4",
    "@inquirer/select": "^5.0.4",
    "chalk": "^5.6.2"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "lefthook"
    ]
  }
}
