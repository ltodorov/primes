{
  "name": "@ltodorov/primes",
  "version": "2.0.1",
  "description": "Utility functions for working with prime numbers",
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
  "main": "./dist/index.js",
  "bin": "./index.js",
  "scripts": {
    "clean": "pnpm run /^clean:.+/",
    "clean:build": "rimraf ./dist",
    "clean:test": "rimraf ./coverage",
    "build": "pnpm clean:build && tsc",
    "lint": "pnpm biome lint --write .",
    "format": "pnpm biome format --write .",
    "check": "pnpm biome check --write .",
    "check:ci": "pnpm biome ci .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ltodorov/primes.git"
  },
  "bugs": {
    "url": "https://github.com/ltodorov/primes/issues"
  },
  "homepage": "https://github.com/ltodorov/primes",
  "keywords": [
    "typescript",
    "javascript",
    "nodejs",
    "prime numbers"
  ],
  "author": "Lyubomir Todorov",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@inquirer/testing": "^3.0.1",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.14",
    "lefthook": "^2.0.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "@inquirer/input": "^5.0.1",
    "@inquirer/select": "^5.0.1",
    "chalk": "^5.6.2"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
